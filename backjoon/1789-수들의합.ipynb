    "result = []\n",
    "sum = 0\n",
    "curNum = 1\n",
    "\n",
    "while True:\n",
    "    sum += curNum\n",
    "    if s==sum:\n",
    "        result.append(curNum)\n",
    "        break\n",
    "    elif sum < s:\n",
    "        result.append(curNum)\n",
    "        curNum+=1\n",
    "    else:\n",
    "        break\n",
    "        \n",
    "    \n",
